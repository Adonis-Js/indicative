{
  "name": "indicative",
  "version": "5.0.8",
  "description": "Concise data validation library for Node.js and browsers",
  "main": "builds/main.js",
  "files": [
    "builds"
  ],
  "scripts": {
    "mrm": "mrm --preset=@adonisjs/mrm-preset",
    "build": "npm run compile",
    "prepublishOnly": "npm run build",
    "docs": "node bin/inlineDocs && cd static && chul build",
    "docs:serve": "node bin/inlineDocs && cd static && chul serve",
    "pretest": "npm run lint",
    "test": "node japaFile.js",
    "lint": "tslint --project tsconfig.json",
    "clean": "del build",
    "compile": "npm run lint && npm run clean && tsc",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "author": "virk",
  "license": "MIT",
  "devDependencies": {
    "@adonisjs/mrm-preset": "^1.0.16",
    "@types/node": "^11.9.5",
    "chalk": "^2.4.2",
    "chul": "^1.0.3",
    "coveralls": "^3.0.3",
    "cz-conventional-changelog": "^2.1.0",
    "del-cli": "^1.1.0",
    "fs-extra": "^7.0.1",
    "got": "^9.6.0",
    "japa": "^2.0.7",
    "japa-cli": "^1.0.1",
    "klaw": "^3.0.0",
    "mrm": "^1.2.1",
    "nyc": "^13.3.0",
    "opn": "^5.4.0",
    "pkg-ok": "^2.3.1",
    "ts-node": "^8.0.2",
    "tslint": "^5.13.0",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^3.3.3333"
  },
  "dependencies": {
    "@slynova/slug": "^1.1.2",
    "clone": "^2.1.2",
    "date-fns": "^1.30.1",
    "haye": "^2.0.2",
    "p-lazy": "^1.0.0",
    "pluralize": "^7.0.0",
    "pope": "^2.0.2",
    "striptags": "^3.1.1",
    "validator": "^10.11.0"
  },
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/poppinss/indicative.git"
  },
  "keywords": [
    "node-validator",
    "validator",
    "schema-validator",
    "quick-validations"
  ],
  "bugs": {
    "url": "https://github.com/poppinss/indicative/issues"
  },
  "homepage": "https://github.com/poppinss/indicative#readme",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "standard": {
    "globals": [
      "test",
      "group",
      "QUnit"
    ]
  },
  "nyc": {
    "exclude": [
      "test"
    ],
    "extension": [
      ".ts"
    ]
  }
}
