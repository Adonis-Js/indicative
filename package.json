    "coveralls": "^2.11.4",
    "istanbul": "^0.4.0",
    "mocha": "^2.3.3",
    "mocha-lcov-reporter": "^1.0.0"
  },
  "dependencies": {
    "bluebird": "^3.0.5",
    "dot-prop": "^2.2.0",
    "inflect": "^0.3.0",
    "moment": "^2.10.6",
    "pope": "^1.0.0"
  },
  "directories": {
    "doc": "docs",
    "test": "test"
  },
  "repository": {
