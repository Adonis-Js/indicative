{
  "name": "indicative",
  "version": "5.0.8",
  "description": "Simple yet powerful data validation library for Nodejs",
  "main": "build/index.js",
  "files": [
    "build/src",
    "build/index.js",
    "build/index.d.ts"
  ],
  "scripts": {
    "pretest": "npm run lint",
    "test": "nyc node japaFile.js",
    "lint": "tslint --project tsconfig.json",
    "clean": "del build",
    "compile": "npm run lint && npm run clean && webpack",
    "build": "npm run compile",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "keywords": [
    "utils",
    "indicative"
  ],
  "author": "virk",
  "homepage": "https://indicative.adonisjs.com",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/poppinss/indicative/tree/master/packages/indicative.git"
  },
  "license": "MIT",
  "nyc": {
    "exclude": [
      "test"
    ],
    "extension": [
      ".ts"
    ]
  },
  "publishConfig": {
    "access": "public",
    "tag": "next"
  },
  "bugs": {
    "url": "https://github.com/poppinss/indicative/issues"
  }
  "dependencies": {
    "indicative-compiler": "^5.0.8",
    "indicative-rules": "^5.0.8",
    "indicative-formatters": "^5.0.8"
  },
  "devDependencies": {
    "benchmark": "^2.1.4"
  }
}
