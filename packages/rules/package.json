{
  "name": "indicative-rules",
  "version": "5.0.8",
  "description": "Fluent data validator",
  "main": "build/index.js",
  "files": [
    "build/src",
    "build/index.js",
    "build/index.d.ts"
  ],
  "scripts": {
    "pretest": "npm run lint",
    "test": "nyc node japaFile.js",
    "lint": "tslint --project tsconfig.json",
    "clean": "del build",
    "compile": "npm run lint && npm run clean && webpack",
    "build": "npm run compile",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "keywords": [
    "parser",
    "indicative"
  ],
  "author": "virk",
  "homepage": "https://indicative.adonisjs.com",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/poppinss/indicative/tree/master/packages/indicative.git"
  },
  "license": "MIT",
  "nyc": {
    "exclude": [
      "test"
    ],
    "extension": [
      ".ts"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/poppinss/indicative/issues"
  },
  "devDependencies": {
    "indicative-compiler": "^5.0.8"
  },
  "dependencies": {
    "date-fns": "^1.30.1",
    "validator": "^10.11.0"
  }
}
